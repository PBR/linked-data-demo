select distinct
    (?placeholder as ?url)
    (?bstatus as ?value)
    #?acc
from ds1:#
from ds2:#
from ds3:#
from ds4:#
from ds5:#
from ds6:#
from ds7:#
where {
  ?acc a WUR_ont:id .
  ?orPlant WUR_ont:has_id ?acc .
  ?orPlant WUR_ont:has_biological_status ?bstatusURI .
  ?bstatusURI rdfs:label ?bstatus .
}
